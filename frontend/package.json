{
  "name": "yav-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "yarn prisma:generate",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prisma:seed": "dotenv -e .env.local -- yarn prisma db seed",
    "prisma:studio": "dotenv -e .env.local -- yarn prisma studio",
    "prisma:dbpush": "dotenv -e .env.local -- yarn prisma db push",
    "prisma:generate": "dotenv -e .env.local -- yarn prisma generate --generator client --no-engine",
    "prisma:generatepy": "dotenv -e .env.local -- prisma generate --generator scraper --no-engine"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@prisma/extension-accelerate": "^0.6.2",
    "classnames": "^2.3.2",
    "code-tag": "^1.1.0",
    "debounce": "^1.2.1",
    "jotai": "^2.5.0",
    "next": "^14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-force-graph-2d": "^1.25.2",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-wrap-balancer": "^1.1.0",
    "transition-hook": "^1.5.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@next/bundle-analyzer": "^14.0.0",
    "@swc-jotai/react-refresh": "^0.1.0",
    "@types/debounce": "^1.2.3",
    "@types/node": "^20.8.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-css-order": "^2.0.1",
    "prisma": "^5.5.2",
    "sass": "^1.69.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
