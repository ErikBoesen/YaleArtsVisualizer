    "next": "^14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-force-graph-2d": "^1.25.2",
    "react-icons": "^4.11.0",
    "swr": "^2.2.4",
    "transition-hook": "^1.5.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@next/bundle-analyzer": "^14.0.0",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-css-order": "^2.0.1",
